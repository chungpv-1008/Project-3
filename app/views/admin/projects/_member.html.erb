<tr>
  <th scope="row"><%= member_counter + 1 %></th>
  <td><%= member.name %></td>
  <td><%= member.email %></td>
  <td><%= member.role %></td>
  <td>
    <%= link_to admin_user_project_path(UserProject.find_by(user: member, project: @project)), method: :delete, data: {toggle: "tooltip", placement: :top, confirm: "Are you sure you?"}, class: "btn btn-danger" do %>
      Out project
      <i class="mdi mdi-forward mdi-9px"></i>
    <% end %>
  </td>
</tr>
