<% if project.status == "unlock_" %>
  <button type="button" class="btn btn-success dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" disabled="disabled">
    Download data
    <i class="mdi mdi-database-export mdi-9px"></i>
  </button>
<% else %>
  <button type="button" class="btn btn-success dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" disabled="disabled"> 
    Download data
    <i class="mdi mdi-database-export mdi-9px"></i>
  </button>
<% end %>
<div class="dropdown-menu">
  <%= link_to "CSV", "http://localhost:3000/en/locations.csv?project_id=#{project.id}", class: "dropdown-item" %>
  <div class="dropdown-divider"></div>
  <%= link_to "JSON", "http://localhost:3000/en/locations.json?project_id=#{project.id}", class: "dropdown-item" %>
</div>
