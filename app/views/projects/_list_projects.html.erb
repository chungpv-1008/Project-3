<tr class="row">
  <th scope="row" class="col-md-1 table-project"><%= project_counter + 1 %></th>
  <td class="col-md-3 table-project"><%= image_tag project.avatar, class: "avatar-project-row" if project.avatar.attached? %></td>
  <td class="col-md-3 table-project"><%= project.name %></td>
  <td class="col-md-5 table-project">
    <%= link_to project_path(project), class: "btn btn-success" do %>
      Detail
      <i class="mdi mdi-forward mdi-9px"></i>
    <% end %>
  </td>
</tr>
