<%= provide :title, t(".title") %>
<%= provide :submit, t(".submit") %>
<div class="container">
  <div class="row mt-5">
    <h2 class="w-100 text-center">
      <%= t ".title" %>
    </h2>
  </div>
  <div class="row mb-5">
    <div class="col-2">
    </div>
    <div class="col-8">
      <%= render partial: "videos/common/form", locals: {video: @video, course: @course} %>
      <hr>
      <%= link_to t(".back"), @course, class: "btn btn-primary" %>
    </div>
    <div class="col-2">
    </div>
  </div>
</div>
