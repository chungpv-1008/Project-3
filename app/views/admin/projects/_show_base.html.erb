<div class="w-100">
  <div class="row mt-5">
    <div class="col-1"></div>
    <div class="col-2">
      <%= image_tag project.avatar, class: "myImg avatar-project-row", id: "myImg" if project.avatar.attached? %>
    </div>
    <div class="col-4 detail-project-name">
      <%= link_to project.name, admin_project_path(project) %>
    </div>
  </div>
  <div class="row mt-5">
    <div class="col-1"></div>
    <div class="col-2">
      <%= link_to members_admin_project_path(project) do %>
        <%= project.users.size %> members
      <% end %>
    </div>
    <div class="col-2">
      <%= link_to images_admin_project_path(project) do %>
        <%= project.images.size %> images
      <% end %>
    </div>
    <div class="col-2" id=<%= "project-status-#{project.id}" %> >
      <%= render partial: "admin/projects/status", locals:{project: project} %>
    </div>
    <div class="col-2 btn-group" id=<%= "project-download-#{project.id}" %>>
      <%= render partial: "admin/projects/download", locals:{project: project} %>
    </div>
  </div>
</div>
